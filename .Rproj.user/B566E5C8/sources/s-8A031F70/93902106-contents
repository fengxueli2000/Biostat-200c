count = 1
for i in [3, 6, 4, 32, 3]:
    print('\n现在执行第{}次循环，输出的值是：{}'.format(count, i))
    count = count + 1




sum = 0
for x in range(5):
    sum = x + sum
    print('\nthis is {} times ，sum current value：{}'.format(x + 1, sum))#不想看这个可以不写这个print
print('\ntotal value??<9a>%d' % sum)



#while loop

#while format()


tempreture = 0  # 设置变量 并赋值为0
while (tempreture <=105):  # 若温??<a6><=105度，则继续执行循??<af>
    print('你的体温正在上升：{}'.format(tempreture))
    tempreture = tempreture + 15  # 每一次循环，变量都增??<a0>15
print("\n我热爱{}°C的你".format(tempreture - 15))



